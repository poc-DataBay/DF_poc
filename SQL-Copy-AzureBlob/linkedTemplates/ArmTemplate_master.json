{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SQL-Copy-AzureBlob"
		},
		"AzSQLWorkshop_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzSQLWorkshop'"
		},
		"LS_DataLake_pocDataBay_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://pocdatabay.azuredatalakestore.net/webhdfs/v1"
		},
		"LS_DataLake_pocDataBay_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "95128928-d38b-40cd-a9f9-3d28564cd755"
		},
		"LS_DataLake_pocDataBay_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "0e009c97-b66b-4a12-ba88-aee0f9f426d3"
		},
		"LS_DataLake_pocDataBay_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "DataFactoryRG"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SQL-Copy-AzureBlob_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzSQLWorkshop_connectionString": {
						"value": "[parameters('AzSQLWorkshop_connectionString')]"
					},
					"LS_DataLake_pocDataBay_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('LS_DataLake_pocDataBay_properties_typeProperties_dataLakeStoreUri')]"
					},
					"LS_DataLake_pocDataBay_properties_typeProperties_tenant": {
						"value": "[parameters('LS_DataLake_pocDataBay_properties_typeProperties_tenant')]"
					},
					"LS_DataLake_pocDataBay_properties_typeProperties_subscriptionId": {
						"value": "[parameters('LS_DataLake_pocDataBay_properties_typeProperties_subscriptionId')]"
					},
					"LS_DataLake_pocDataBay_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('LS_DataLake_pocDataBay_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}